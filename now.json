{
    "version": 2,
    "alias": "primitivedigital.co.uk",
    "builds": [
      {
        "src": "build/static/**/*",
        "use": "@now/static"
      },
      {"src": "static/*", "use": "@now/static"},
      {
        "src": "favicon.ico",
        "use": "@now/static"
      },
      {
        "src": "build/server.js",
        "use": "@now/node"
      },
  
      {
        "src": "package.json",
        "use": "@now/node-server"
      }
    ],
    "routes": [	
      { "src": "/301",
        "status": 301,
        "headers": { "Location": "https://primitivedigital.co.uk/" }
      },
      { "src": "/redirect",
        "dest": "https://primitivedigital.co.uk/"
      },
      {
        "src": "/static/(.*)",
        "dest": "/build/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/server.js"
      }
      ]
  }