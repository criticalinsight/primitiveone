{
    "version": 2,
    "alias": "primitivedigital.co.uk",
    "builds": [
      {
        "src": "package.json",
        "use": "@frontity/now"
      },
      {
        "src": "build/static/**/*",
        "use": "@now/static"
      }
    ],
    "routes": [	
      { "src": "/301",
        "status": 301,
        "headers": { "Location": "https://primitivedigital.co.uk/" }
      },
      { "src": "/redirect",
        "dest": "https://primitivedigital.co.uk/"
      },
      {
        "src": "/static/(.*)",
        "dest": "/build/static/$1"
      }
      ]
  }