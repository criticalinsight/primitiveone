{
    "version": 2,
    "alias": "primitivedigital.co.uk",
    "builds": [
      {
        "src": "package.json",
        "use": "@frontity/now"
      }
    ],
    "routes": [	
      { "src": "/?test=test",
        "status": 301,
        "headers": { "Location": "https://primitivedigital.co.uk/" }
      },
      { "src": "/301",
        "status": 301,
        "headers": { "Location": "https://primitivedigital.co.uk/" }
      },
      { "src": "/redirect", "dest": "https://primitivedigital.co.uk/" }
      ]
  }