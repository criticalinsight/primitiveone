{
  "version": 2,
  "alias": "primitivedigital.co.uk",
  "builds": [
    {
      "src": "package.json",
      "use": "@frontity/now"
    }
  ],
  "routes": [
    {
      "src": "/page301",
      "status": 301,
      "headers": {
        "Location": "https://primitivedigital.co.uk/blog/primitiveone"
      }
    },
    {
      "src": "/302-test",
      "dest": "https://primitivedigital.co.uk/stats"
    },
    {
      "src": "/sitemap.xml",
      "status": 301,
      "headers": {
        "Location": "https://primitivedigital.co.uk/sitemap_index.xml"
      }
    },
    {
      "src": "/sitemap_index.xml",
      "dest": "https://api.primitivedigital.uk/sitemap_index.xml"
    }

    
  ]
}