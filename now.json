{
  "version": 2,
  "alias": "primitivedigital.co.uk",
  "builds": [
    {
      "src": "package.json",
      "use": "@frontity/now"
    }
  ],
  "routes": [
    {
      "src": "/page301",
      "status": 301,
      "headers": {
        "Location": "https://primitivedigital.co.uk/blog/primitiveone"
      }
    },
    {
      "src": "/redirect",
      "dest": "https://primitivedigital.co.uk/stats"
    },
    {
      "src": "/sitemap.xml",
      "status": 301,
      "headers": {
        "Location": "https://primitivedigital.co.uk/sitemap_index.xml"
      }
    },
    {
      "src": "/wp-admin/",
      "dest": "https://wp.primitivedigital.uk/wp-admin/"
    },
    {
      "src": "/wp-login.php",
      "dest": "https://wp.primitivedigital.uk/wp-login.php"
    },
    {
      "src": "/sitemap_index.xml",
      "dest": "https://wp.primitivedigital.uk/sitemap_index.xml"
    }

    
  ]
}