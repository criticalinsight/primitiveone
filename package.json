{
  "name": "primitiveone",
  "version": "1.0.4",
  "private": true,
  "description": "Primitive One WordPress + Frontity Theme",
  "keywords": [
    "frontity",
    "primitivedigital",
    "headless",
    "decoupled",
    "rest",
    "theme"
  ],
  "scripts": {
    "dev": "frontity dev",
    "dev:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity dev",
    "build": "frontity build",
    "build:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity build",
    "serve": "frontity serve",
    "serve:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity serve"
  },
  "prettier": {},
  "dependencies": {
    "@frontity/core": "^1.3.0",
    "@frontity/html2react": "^1.1.12",
    "@frontity/primitiveone": "file:packages/primitiveone",
    "@frontity/tiny-router": "^1.0.15",
    "@frontity/wp-source": "^1.4.0",
    "frontity": "^1.4.0"
  }
}
