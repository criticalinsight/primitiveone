{
  "name": "primitiveone",
  "version": "1.0.1",
  "private": true,
  "description": "Primitive One Theme - Built with Frontity",
  "keywords": [
    "frontity"
  ],
  "scripts": {
    "dev": "frontity dev",
    "dev:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity dev",
    "build": "frontity build",
    "build:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity build",
    "serve": "frontity serve",
    "serve:inspect": "node --inspect -r ts-node/register/transpile-only ./node_modules/.bin/frontity serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/primitiveshaun/primitiveone.git"
  },
  "author": "Shaun Knowles",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/primitiveshaun/primitiveone/issues"
  },
  "homepage": "https://github.com/primitiveshaun/primitiveone",
  "prettier": {},
  "dependencies": {
    "@frontity/core": "^1.2.1",
    "@frontity/html2react": "^1.1.11",
    "@frontity/mars-theme": "./packages/mars-theme",
    "@frontity/primitiveone": "file:packages/primitiveone",
    "@frontity/tiny-router": "^1.0.14",
    "@frontity/wp-source": "^1.3.1",
    "bootstrap": "^4.3.1",
    "frontity": "^1.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-facebook": "^8.0.4",
    "react-ga": "^2.6.0",
    "react-gsap": "^1.2.0",
    "react-scrollmagic": "^2.1.1"
  }
}
